    <div class="text-center">
      <h1>レビュー一覧</h1>
        <table class="table table-striped table-bordered">
          <% @reviews.each do |reviews| %>
            <tr><td><%= link_to reviews.title, reviews %></td></tr>
            <tr><td><%= reviews.comment %></td></tr>
          <% end %>
        </table>
    </div>